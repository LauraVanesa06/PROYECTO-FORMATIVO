<div id="<%= dom_id product %>">
  <p>
    <strong>Nombre:</strong>
    <%= product.nombre %>
  </p>

  <p>
    <strong>Descripcion:</strong>
    <%= product.descripcion %>
  </p>

  <p>
    <strong>Precio:</strong>
    <%= product.precio %>
  </p>

  <p>
    <strong>Stock:</strong>
    <%= product.stock %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= product.category_id_id %>
  </p>

  <p>
    <strong>Supplier:</strong>
    <%= product.supplier_id_id %>
  </p>

</div>
